package day01_basic; //package 이름

public class Hello {	// class 이름
	public static void main(String[] args) {
	// 메소드 선언
	// main 입력 + ctrl + space bar : 자동완성
		
		
		System.out.println("\n Hello");
		// sysout 입력 + ctrl + space bar
		// \n은 한 칸 띄어쓰기
		System.out.print("Hello");
		// System.out.print은 붙여쓰기
		System.out.println("Hello");
		// System.out.println은 한 줄 띄어쓰기
		
	
	} // main end
} // class end
